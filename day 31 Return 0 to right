
#include <iostream>
using namespace std;

int sort(int array[], int number)
{
    int num =0;
    for(int i=0; i<number; i++)
    {
        if(array[i] == 0)
        {
            num++;
            continue;
        }
        cout<<array[i]<<" ";
    }
    for(int j=0; j<num; j++)
    {
        cout<<0<<" ";
    }
    return 0;
}
int main()
{
    int array[10]= {12,32,43,0,0,2,0,89,75,0};
    int number = 10;
    sort(array, number);

    return 0;
}
